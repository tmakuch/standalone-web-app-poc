{
  "name": "standalone-web-app-poc",
  "version": "0.0.1",
  "scripts": {
    "start": "npm-run-all --parallel continuous-build serve-build",
    "clean-pre-build": "rimraf dist",
    "clean-post-build": "rimraf dist/styles*.js*",
    "build": "npm run clean-pre-build && cross-env NODE_ENV=prod webpack --bail && npm run clean-post-build",
    "continuous-build": "cross-env NODE_ENV=dev webpack",
    "serve-build": "browser-sync start --server dist --files dist"
  },
  "author": "tmakuch <tadeusz.makuch@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^2.18.13",
    "copy-webpack-plugin": "^4.1.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "json-loader": "^0.5.7",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "npm-run-all": "^4.1.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tmakuch/standalone-web-app-poc.git"
  },
  "bugs": {
    "url": "https://github.com/tmakuch/standalone-web-app-poc/issues"
  },
  "homepage": "https://github.com/tmakuch/standalone-web-app-poc#readme",
  "description": ""
}
